@prefix bgf: <https://bg-rdf.org/ontologies/bondgraph-framework#> .
@prefix cdt: <https://w3id.org/cdt/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

@prefix : <https://bg-rdf.org/models/local/circulation#> .
@prefix circ: <https://bg-rdf.org/models/local/circulation#> .
@prefix gi-tract: <https://bg-rdf.org/models/local/gi-tract#> .
@prefix kidney: <https://bg-rdf.org/models/local/kidney#> .

#===============================================================================

:circulation
    a bgf:BondgraphModel ;
    bgf:usesTemplate
        '../../BG-RDF/schema/elements/chemical.ttl' ,
        '../../BG-RDF/schema/elements/hydraulic.ttl' ;
    bgf:hasBondElement
        :q_vc_W, :v_lv_W,
        :q_ac_W, :v_cc_W,
        :q_vc_Na .

#===============================================================================
#===============================================================================

:q_vc_W
    a bgf:HydraulicElasticStorageNode ;
    bgf:hasValue "2 L"^^cdt:ucum ;
    bgf:parameterValue [
        bgf:varName "E" ;
        bgf:hasSymbol "E_vc" ;
        bgf:hasValue "6e1 J"^^cdt:ucum
    ], [
        bgf:varName "q_hat" ;
        bgf:hasSymbol "U_vc_W" ;
        bgf:hasValue "1.5 L"^^cdt:ucum
    ], [
        bgf:varName "q_bar" ;
        bgf:hasSymbol "L_vc_W" ;
        bgf:hasValue "2.5 L"^^cdt:ucum
    ] .

#===============================================================================

:v_lv_W
    a bgf:HydraulicFlow ;
    bgf:hasValue "0.1 L/s"^^cdt:ucum .

#===============================================================================

:q_ac_W
    a bgf:HydraulicElasticStorageNode ;
    bgf:hasValue "1 L"^^cdt:ucum ;
    bgf:parameterValue [
        bgf:varName "E" ;
        bgf:hasSymbol "E_ac" ;
        bgf:hasValue "6e1 J"^^cdt:ucum
    ], [
        bgf:varName "q_hat" ;
        bgf:hasSymbol "U_ac_W" ;
        bgf:hasValue "0.5 L"^^cdt:ucum
    ], [
        bgf:varName "q_bar" ;
        bgf:hasSymbol "L_ac_W" ;
        bgf:hasValue "1.5 L"^^cdt:ucum
    ] .

#===============================================================================

:v_cc_W
    a bgf:HydraulicResistanceNode ;
    bgf:parameterValue [
        bgf:varName "k" ;
        bgf:hasSymbol "k_cc_W" ;
        bgf:hasValue "1 L.s-1.kPa-1"^^cdt:ucum
    ] .

#===============================================================================

:q_vc_Na
    a bgf:ChemicalStorageNode ;
    bgf:hasValue "0.84 mol"^^cdt:ucum ;
    bgf:parameterValue [
        bgf:varName "K" ;
        bgf:hasValue "1 mol-1"^^cdt:ucum
    ] .

#===============================================================================
#===============================================================================

:circ_bond_1
    bgf:hasSource [
        bgf:element gi-tract:v_giEpi_NKE ;
        bgf:port 'Na_r'
    ] ;
    bgf:hasTarget :q_vc_Na .

:circ_bond_2
    bgf:hasSource [
        bgf:element kidney:v_ptEpi_NKE ;
        bgf:port 'Na_r'
    ] ;
    bgf:hasTarget :q_vc_Na .

:circ_bond_3
    bgf:hasSource :q_vc_Na ;
    bgf:hasTarget [
        bgf:element kidney:v_gl_Na ;
        bgf:port 'f'
    ] .

:circ_bond_4
    bgf:hasSource gi-tract:v_gi_W ;
    bgf:hasTarget :q_vc_W .

:circ_bond_5
    bgf:hasSource kidney:v_pt_W ;
    bgf:hasTarget :q_vc_W .

:circ_bond_6
    bgf:hasSource :v_cc_W ;
    bgf:hasTarget :q_vc_W .

:circ_bond_7
    bgf:hasSource :q_vc_W ;
    bgf:hasTarget :v_lv_W .

:circ_bond_8
    bgf:hasSource :v_lv_W ;
    bgf:hasTarget :q_ac_W .

:circ_bond_9
    bgf:hasSource :q_ac_W ;
    bgf:hasTarget kidney:v_gl_W .

:circ_bond_9
    bgf:hasSource :q_ac_W ;
    bgf:hasTarget :v_cc_W .

#===============================================================================
#===============================================================================
